{
  "name": "titlebar-launcher",
  "version": "0.0.1",
  "description": "adds a launcher bar on the titlebar",
  "main": "./out/index.js",
  "vortex": {
    "version": ">=1.2.0"
  },
  "scripts": {
    "webpack": "webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "installDev": "copyfiles -f dist/* ../../out/bundledPlugins/titlebar-launcher",
    "build": "npm run webpack && copyfiles -f titlebar-launcher.scss dist && copyfiles -f src/images/* dist && extractInfo",
    "buildDev": "npm run build && npm run installDev"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-bootstrap": "^0.32.20",
    "@types/node": "^13.5.1",
    "copyfiles": "^2.2.0",
    "memoize-one": "^5.1.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vortex-api": "Nexus-Mods/vortex-api#electron7",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
